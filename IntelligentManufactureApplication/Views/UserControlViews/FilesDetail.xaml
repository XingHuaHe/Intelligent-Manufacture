                    </DataGrid>

                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 2 0 2">
                        <!--<Button x:Name="DrawingPrintButton" Content="打印" Width="60" Height="25" Margin="5 0 5 0" BorderBrush="#0A0C22" Background="Transparent" Foreground="Black"/>-->
