                        </materialDesign:DataGridTextColumn>

                        <materialDesign:DataGridTextColumn Header="创建人" ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}" EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnPopupEditingStyle}">
                            <materialDesign:DataGridTextColumn.Binding>
                                <Binding Path="ModifiedUser">
                                    <Binding.ValidationRules>
                                        <service:NotEmptyValidationRule />
                                    </Binding.ValidationRules>
                                </Binding>
                            </materialDesign:DataGridTextColumn.Binding>
                        </materialDesign:DataGridTextColumn>

                        <materialDesign:DataGridTextColumn Header="创建时间" ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}" EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnPopupEditingStyle}">
                            <materialDesign:DataGridTextColumn.Binding>
                                <Binding Path="ModifiedTimee">
                                    <Binding.ValidationRules>
                                        <service:NotEmptyValidationRule />
                                    </Binding.ValidationRules>
                                </Binding>
                            </materialDesign:DataGridTextColumn.Binding>
                        </materialDesign:DataGridTextColumn>
                        
                    </DataGrid.Columns>
                </DataGrid>

                <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right">
                    <!--<Button x:Name="PrintNCButton" Content="打印" Width="60" Height="25" Margin="5 0 5 0" BorderBrush="#0A0C22" Background="Transparent" Foreground="Black"/>-->
                    <Button x:Name="PreviewNCButton" Content="预览" Width="60" Height="25" Margin="5 0 5 0" BorderBrush="#0A0C22" Background="Transparent" Foreground="Black" Click="PreviewNCButton_Click" />
                    <Button x:Name="DeletedNCButton" Content="删除" Width="60" Height="25" Margin="5 0 5 0" BorderBrush="#0A0C22" Background="Transparent" Foreground="Black" Click="DeletedNCButton_Click" />
                    <Button x:Name="AddNCButton" Content="导入" Width="60" Height="25" Margin="5 0 7 0" BorderBrush="#0A0C22" Background="Transparent" Foreground="Black" Click="AddNCButton_Click" />
                </StackPanel>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
