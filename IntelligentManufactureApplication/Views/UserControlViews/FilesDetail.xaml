                        </DataGrid.Resources>

                        <DataGrid.Columns>
                            <materialDesign:DataGridTextColumn Header="图纸名称" ElementStyle="{StaticResource MaterialDesignDataGridTextColumnStyle}" EditingElementStyle="{StaticResource MaterialDesignDataGridTextColumnPopupEditingStyle}">
